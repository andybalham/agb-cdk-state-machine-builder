{
  "mochaExplorer.files": "test/**/*.ts",
  "mochaExplorer.require": "ts-node/register",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.language": "en-GB",
  "workbench.colorCustomizations": {
    "[Default Dark+]": {
      "titleBar.activeBackground": "#850021",
      "sideBar.background": "#540015",
      "activityBar.background": "#850021",
      "activityBar.activeBackground": "#b5002d"
    }
  },
  "cSpell.words": [
    "Chainable",
    "Nextable",
    "istanbuljs",
    "npmrc",
    "stepfunctions"
  ]
}